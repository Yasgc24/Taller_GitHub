Proceso CiudadCapital
	Definir ciudad_capital Como Caracter;
	Definir pais Como Caracter;
	
	Escribir "Escriba una ciudad capital de un país";
	Leer ciudad_capital;
	Escribir "Escriba el país";
	Leer pais;
	
	Limpiar Pantalla;
	
	Escribir "La ciudad ", ciudad_capital, ", es la capital del país ", pais;
	
FinProceso
